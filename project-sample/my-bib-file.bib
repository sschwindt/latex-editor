  author = {Grygoruk, Mateusz and Sza{\l}kiewicz, Ewelina and {Grodzka-{\L}ukaszewska}, Maria and {Miros{\l}aw-{\'S}wi{\k{a}}tek}, Dorota and Ogl{\k{e}}cki, Pawe{\l} and {Pus{\l}owska-Tyszewska}, Dorota and Sinicyn, Grzegorz and Okruszko, Tomasz},
