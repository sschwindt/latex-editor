@article{cordier_bar_2020,
  title = {Bar Pattern and Sediment Sorting in a Channel Contraction/Expansion Area: {{Application}} to the {{Loire River}} at {{Br\'eh\'emont}} ({{France}})},
  shorttitle = {Bar Pattern and Sediment Sorting in a Channel Contraction/Expansion Area},
