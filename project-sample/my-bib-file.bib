@article{kirillin_generalized_2016,
  title = {Generalized Scaling of Seasonal Thermal Stratification in Lakes},
  author = {Kirillin, G. and Shatwell, T.},
  year = {2016},
  month = oct,
  journal = {Earth-Science Reviews},
  volume = {161},
  pages = {179--190},
  issn = {0012-8252},
  doi = {10.1016/j.earscirev.2016.08.008},
  abstract = {Hutchinson and L\"offler's (1956) classification of lakes based on the seasonal thermal mixing regime has become a cornerstone of any analysis of lakes as elements of the earth surface. Until now however the lake classification has lacked a physically sound quantitative criterion distinguishing between two fundamental lake types: thermally stratified during a large portion of the year (mono- and dimictic) and predominantly mixed to the bottom (polymictic). Using the mechanistic balance between potential and kinetic energy we review the different formulations of the Richardson number to derive a generalized scaling for seasonal stratification in a closed lake basin. The scaling parameter is the critical mean basin depth, Hcrit, that delineates lakes that mix regularly from those that stratify seasonally based on lake water transparency, lake length, and an annual mean estimate for the Monin-Obukhov length. We validate the scaling on available data of lakes worldwide using logistic regression. The scaling criterion consistently described the mixing regime significantly better than either the conventional unbounded basin scaling or a simple depth threshold. Thus, the generalized scaling is universal for freshwater lakes and allows the seasonal mixing regime to be estimated without numerically solving the heat transport equations.},
  langid = {english},
  keywords = {Dimixis,Lake classification,Lake databases,Lake modeling,Polymixis,Richardson number,Seasonal stratification,Secchi depth,Water transparency},
  file = {/home/schwindt/Zotero/storage/IH4YJAFE/Kirillin and Shatwell - 2016 - Generalized scaling of seasonal thermal stratifica.pdf;/home/schwindt/Zotero/storage/2U4W7RSM/S0012825216302379.html}
}

@book{kitanidis_introduction_1997,
