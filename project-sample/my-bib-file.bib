@article{zanden_global_2007,
  title = {Global Patterns of Aquatic Food Chain Length},
  author = {Zanden, M. Jake Vander and Fetzer, William W.},
  year = {2007},
  journal = {Oikos},
  volume = {116},
  number = {8},
  pages = {1378--1388},
  issn = {1600-0706},
  doi = {10.1111/j.0030-1299.2007.16036.x},
  abstract = {Food chain length is a fundamental ecosystem property, and plays a central role in determining ecosystem functioning. Recent advances in the field of stable isotope ecology allow the estimation of food chain length (FCL) from stable nitrogen isotope ({$\delta$}15N) data. We conducted a global literature synthesis and estimated FCL for 219 lake, stream, and marine ecosystems. Streams had shorter food chains ({$\sim$}3.5 trophic levels) than marine and lake ecosystems ({$\sim$}4.0 trophic levels). In marine systems, inclusion of marine mammals increased FCL by 2/3 of a trophic level. For each ecosystem type, estimates of FCL were normally distributed and spanned two full trophic levels. Comparison with published connectance food webs revealed similar mean FCL values, though stable isotope-derived FCL estimates were less variable. At the global scale, FCL showed weak or no relationships with ecosystem size, mean annual air temperature, or latitude. Our study highlights the utility of stable isotopes for quantifying among-system food web variability, and the application of this approach for assessing global-scale patterns of food chain length.},
  langid = {english},
  annotation = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/j.0030-1299.2007.16036.x},
  file = {/home/schwindt/Zotero/storage/E5V3HZER/j.0030-1299.2007.16036.html}
}

