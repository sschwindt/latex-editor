  langid = {english},
  keywords = {clogging,colmation,connectivity,riverbed,vertical},
  file = {/home/schwindt/Zotero/storage/6WVKGB9Z/Grygoruk et al. - 2021 - Revealing the influence of hyporheic water exchang.pdf;/home/schwindt/Zotero/storage/9RNKTRWH/kmae210031.html}
