@article{tonina_evaluating_2020,
  title = {Evaluating the Performance of Topobathymetric {{LiDAR}} to Support Multi-Dimensional Flow Modelling in a Gravel-Bed Mountain Stream},
  author = {Tonina, Daniele and McKean, James A. and Benjankar, Rohan M. and Yager, Elowyn and Carmichael, Richard A. and Chen, Qiuwen and Carpenter, Aston and Kelsey, Leah G. and Edmondson, Michael R.},
  year = {2020},
  journal = {Earth Surface Processes and Landforms},
  volume = {45},
  number = {12},
  pages = {2850--2868},
  issn = {1096-9837},
  doi = {10.1002/esp.4934},
  abstract = {Stream biophysical processes are commonly studied using multi-dimensional numerical modelling that quantifies flow hydraulics from which parameters such as habitat suitability, stream carrying capacity, and bed mobility are derived. These analyses would benefit from accurate high-resolution stream bathymetries spanning tens of kilometres of channel, especially in small streams or where navigation is difficult. Traditional ground-based survey methods are limited by survey time, dense vegetation and stream access, and are usually only feasible for short reaches. Conversely, airborne topobathymetric LiDAR surveys may overcome these limitations, although limited research is available on how errors in LiDAR-derived digital elevation models (DEMs) might propagate through flow models. This study investigated the performance of LiDAR-derived topobathymetry in support of multi-dimensional flow modelling and ecohydraulics calculations in two gravel-bedded reaches (approximately 200 m long), one morphologically complex and one morphologically simple, and at the segment scale (32 km-long stream segment) along a 15 m-wide river in central Idaho, USA. We compared metre and sub-metre-resolution DEMs generated from RTK-GPS ground and Experimental Advanced Airborne Research LiDAR-B (EAARL-B) surveys and water depths, velocities, shear stresses, habitat suitability, and bed mobility modelled with two-dimensional (2D) hydraulic models supported by LiDAR and ground-surveyed DEMs. Residual statistics, bias (B), and standard deviation (SD) of the residuals between depth and velocity predicted from the model supported by LiDAR and ground-survey topobathymetries were up to -0.04 (B) and 0.09 m (SD) for depth and -0.09 (B) and 0.20 m s-1 (SD) for velocity. The accuracy (B = 0.05 m), precision (SD = 0.09 m), and point density (1 point m-2) of the LiDAR topobathymetric survey (regardless of reach complexity) were sufficient to support 2D hydrodynamic modelling and derivative stream habitat and process analyses, because these statistics were comparable to those of model calibration with B = 0 m and SD = 0.04 m for water surface elevation and B = 0.05 m s-1 and SD = 0.22 m s-1 for velocity in our investigation. \textcopyright{} 2020 John Wiley \& Sons, Ltd.},
  langid = {english},
  keywords = {aquatic habitat,ecohydraulic,river morphological complexity,stream hydraulic modelling,topobathymetric LiDAR},
  annotation = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1002/esp.4934},
  file = {/home/schwindt/Zotero/storage/67P8NWBP/esp.html}
}

@incollection{tonina_hydraulic_2013,
  title = {Hydraulic {{Modelling Approaches}} for {{Ecohydraulic Studies}}: {{3D}}, {{2D}}, {{1D}} and {{Non-Numerical Models}}},
  booktitle = {Ecohydraulics},
  author = {Tonina, Daniele and Jorde, Klaus},
  editor = {Maddock, Ian and Harby, Atle and Kemp, Paul and Wood, Paul},
  year = {2013},
  pages = {31--74},
  publisher = {{John Wiley \$\&\$ Sons, Ltd}},
  address = {{Chichester, UK}},
  doi = {10.1002/9781118526576.ch3},
  abstract = {Summary This chapter deals with hydraulic modelling for predicting flow properties used in ecohydraulics, in its broader sense, which encompasses aquatic habitat modelling, ecological assessment and the interaction between flow and organisms. It discusses the basic knowledge and terminology of hydraulic modelling. The chapter focuses on computational fluid dynamics (CFD) modelling and its application in ecohydraulics. It describes the fundamental equations of CFD. The methods for predicting flow properties are divided into four categories: three-dimensional (3D) hydraulic models, two-dimensional (2D) hydraulic models, one-dimensional (1D) hydraulic models and non-numerical hydraulic modelling. The chapter introduces the fundamental topics and concepts of numerical modeling. It describes the features of and differences among hydraulic model approaches and the major elements needed to set up numerical models. The chapter presents four case studies where the underlying hydrodynamic processes drive certain ecological functions, such as the availability of habitats for certain fish or types of vegetation.},
  isbn = {978-1-118-52657-6},
  keywords = {computational fluid dynamics (CFD) modelling,ecohydraulics,non-numerical hydraulic modelling,numerical modeling,one-dimensional (1D) hydraulic models,three-dimensional (3D) hydraulic models,two-dimensional (2D) hydraulic models}
}

